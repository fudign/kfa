# Production Environment Variables for Vercel
# Add these to your Vercel project settings: https://vercel.com/dashboard

# API Configuration
VITE_API_URL=https://kfa-production.up.railway.app/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_PROVIDER=clerk

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true

# i18n
VITE_DEFAULT_LOCALE=ru
VITE_SUPPORTED_LOCALES=ru,ky,en

# Environment
VITE_ENV=production
